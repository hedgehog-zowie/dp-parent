<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>

    <package name="sso" extends="dp-admin" namespace="/sso">

        <action name="index" class="loginAction" method="index">
            <result name="success">/view/frames/admin.jsp</result>
        </action>

        <action name="logout" class="loginAction" method="logout">
        </action>

        <action name="getMenus" class="loginAction" method="getMenus">
            <result name="success">/view/frames/admin_menu.jsp</result>
        </action>

    </package>
</struts>